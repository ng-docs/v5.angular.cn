  "contents": "<article>\n  <header class=\"api-header\">\n    <h1 class=\"no-toc\" id=\"controlcontainer\"><a title=\"Link to this heading\" class=\"header-link\" aria-hidden=\"true\" href=\"api/forms/ControlContainer#controlcontainer\"><i class=\"material-icons\">link</i></a>ControlContainer</h1>\n    <label class=\"api-type-label class\">class</label>\n    \n    \n    \n    <span class=\"version\"></span>\n  </header>\n\n  \n  <!-- INFO BAR -->\n<section class=\"info-bar\">\n\n<table class=\"is-full-width\">\n<tbody>\n<tr>\n  <th>npm Package</th>\n  <td><a href=\"https://www.npmjs.com/package/@angular/forms\">@angular/forms</a></td>\n</tr>\n<tr>\n  <th>Module</th>\n  <td><code>import { <a href=\"api/forms/ControlContainer\" class=\"code-anchor\">ControlContainer</a> } from <a href=\"api/forms\">'@angular/forms'</a>;</code></td>\n</tr>\n<tr>\n  <th>Source</th>\n  <td><a href=\"https://github.com/angular/angular/tree/5.2.11/packages/forms/src/directives/control_container.ts#L11-L32\">forms/src/directives/control_container.ts</a></td>\n</tr>\n\n</tbody>\n</table>\n\n  \n  \n  \n  <section class=\"class-overview\">\n<h2 id=\"overview\"><a title=\"Link to this heading\" class=\"header-link\" aria-hidden=\"true\" href=\"api/forms/ControlContainer#overview\"><i class=\"material-icons\">link</i></a>Overview</h2>\n<code-example language=\"ts\" hidecopy=\"true\">\nclass <a href=\"api/forms/ControlContainer\" class=\"code-anchor\">ControlContainer</a> extends <a class=\"code-anchor\" href=\"api/forms/AbstractControlDirective\">AbstractControlDirective</a> {\n  <a class=\"code-anchor\" href=\"api/forms/ControlContainer#name\">name: string</a>\n  <a class=\"code-anchor\" href=\"api/forms/ControlContainer#formDirective\">get formDirective: Form | null</a>\n  <a class=\"code-anchor\" href=\"api/forms/ControlContainer#path\">get path: string[] | null</a>\n  // inherited from <a class=\"code-anchor\" href=\"api/forms/AbstractControlDirective\">forms/AbstractControlDirective</a>\n