  "contents": "<article>\n  <header class=\"api-header\">\n    <h1 class=\"no-toc\" id=\"numberformatstyle\"><a title=\"Link to this heading\" class=\"header-link\" aria-hidden=\"true\" href=\"api/common/NumberFormatStyle#numberformatstyle\"><i class=\"material-icons\">link</i></a>NumberFormatStyle</h1>\n    <label class=\"api-type-label enum\">enum</label>\n    \n    <label class=\"api-status-label experimental\">experimental</label>\n    \n    <span class=\"version\"></span>\n  </header>\n\n  \n  <!-- INFO BAR -->\n<section class=\"info-bar\">\n\n<table class=\"is-full-width\">\n<tbody>\n<tr>\n  <th>npm Package</th>\n  <td><a href=\"https://www.npmjs.com/package/@angular/common\">@angular/common</a></td>\n</tr>\n<tr>\n  <th>Module</th>\n  <td><code>import { <a href=\"api/common/NumberFormatStyle\" class=\"code-anchor\">NumberFormatStyle</a> } from <a href=\"api/common\">'@angular/common'</a>;</code></td>\n</tr>\n<tr>\n  <th>Source</th>\n  <td><a href=\"https://github.com/angular/angular/tree/5.2.11/packages/common/src/i18n/locale_data_api.ts#L12-L24\">common/src/i18n/locale_data_api.ts</a></td>\n</tr>\n\n</tbody>\n</table>\n\n  \n  \n  \n  <section class=\"enum-overview\">\n<h2 id=\"overview\"><a title=\"Link to this heading\" class=\"header-link\" aria-hidden=\"true\" href=\"api/common/NumberFormatStyle#overview\"><i class=\"material-icons\">link</i></a>Overview</h2>\n<code-example language=\"ts\" hidecopy=\"true\">\nenum <a href=\"api/common/NumberFormatStyle\" class=\"code-anchor\">NumberFormatStyle</a> {\n  <a class=\"code-anchor\" href=\"api/common/NumberFormatStyle#Decimal\">Decimal</a>\n  <a class=\"code-anchor\" href=\"api/common/NumberFormatStyle#Percent\">Percent</a>\n  <a class=\"code-anchor\" href=\"api/common/NumberFormatStyle#Currency\">Currency</a>\n  <a class=\"code-anchor\" href=\"api/common/NumberFormatStyle#Scientific\">Scientific</a>\n}\n</code-example>\n</section>\n\n  \n\n  \n\n\n<section class=\"description\">\n  <h2 id=\"description\"><a ti